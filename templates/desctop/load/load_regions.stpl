<label class="font-semibold">Регион:</label>
<select id="region_id" name="region_id" class="form-select load_region_items form-input mt-2 w-full py-2 px-3 h-10 bg-transparent dark:bg-slate-900 dark:text-slate-200 rounded outline-none border border-gray-200 focus:border-indigo-600 dark:border-gray-800 dark:focus:border-indigo-600 focus:ring-0">
    <option value="">Выберите регион</option>
    <% for object in region_list.iter() { %>
        <option value="<%= object.id %>"><%= object.name %></option>
    <% } %>
</select> 
